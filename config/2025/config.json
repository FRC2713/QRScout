  "sections": [
    {
      "name": "Before Match",
      "fields": [
        {
          "title": "Scouter Initials",
          "description": "Enter the initials of the scouter.",
          "type": "text",
          "required": true,
          "code": "scouter",
          "formResetBehavior": "preserve",
          "defaultValue": ""
        },
        {
          "title": "Match Number",
          "description": "Enter the match number.",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "formResetBehavior": "increment",
          "defaultValue": 1
        },
        {
          "title": "Robot",
          "description": "The robot you are scouting this match, based on driver station position.",
          "type": "select",
          "required": true,
          "code": "robot",
          "formResetBehavior": "preserve",
          "defaultValue": "R1",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          }
        },
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting.",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 11999
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot.",
          "type": "select",
          "required": true,
          "code": "StPo",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Left",
            "R2": "Middle",
            "R3": "Right",
            "R4": "Not Present"
          }
        }
      ]
    },
    {
      "name": "During Match",
      "fields": [
        {
          "title": "Moved in Auto?",
          "description": "Check if the robot moved during autonomous.",
          "type": "boolean",
          "required": false,
          "code": "Mved",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Coral L1 Scored",
          "description": "The number of level 1 coral scored.",
          "type": "counter",
          "required": false,
          "code": "CLO",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Coral L2 Scored",
          "description": "The number of level 2 coral scored.",
          "type": "counter",
          "required": false,
          "code": "CLT",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Coral L3 Scored",
          "description": "The number of level 3 coral scored.",
          "type": "counter",
          "required": false,
          "code": "CLTh",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Coral L4 Scored",
          "description": "The number of level 4 coral scored.",
          "type": "counter",
          "required": false,
          "code": "CLF",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Barge Algae Scored",
          "description": "The number of Algae scored in the barge directly by the robot.",
          "type": "counter",
          "required": false,
          "code": "BAS",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Processor Algae Scored",
          "description": "The number of Algae scored in the processor.",
          "type": "counter",
          "required": false,
          "code": "PAS",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Algae Dislodged",
          "description": "The number of Algae dislodged and not immediately scored.",
          "type": "counter",
          "required": false,
          "code": "AlDl",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        }
      ]
    },
    {
      "name": "After Match",
      "fields": [
        {
          "title": "End Game Result",
          "type": "select",
          "required": true,
          "code": "ega",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "None",
            "P": "Parked",
            "Sc": "Shallow Climb",
            "ScF": "Shallow Climb Failed",
            "Dc": "Deep Climb",
            "DcF": "Deep Climb Failed"
          }
        },
        {
          "title": "Played defense?",
          "type": "boolean",
          "required": false,
          "code": "DfPl",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Defensive Skill",
          "type": "range",
          "required": false,
          "code": "DfSk",
          "formResetBehavior": "reset",
          "defaultValue": 2,
          "min": 1,
          "max": 3,
          "step": 1
        },
        {
          "title": "Finished match?",
          "description": "Set to false if robots tips over, is disabled, etc.",
          "type": "boolean",
          "required": false,
          "code": "Fin",
          "formResetBehavior": "reset",
          "defaultValue": true
        },
        {
          "title": "Notes",
          "description": "Fouls committed, HP notes, etc.",
          "type": "text",
          "required": false,
          "code": "notes",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 300
        }
      ]
    }
  ]
