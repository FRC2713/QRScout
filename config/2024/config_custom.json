          "defaultValue": 0,
          "min": 0,
          "max": 3,
          "required": false
        },
        {
          "title": "End Position",
          "type": "select",
          "required": true,
          "code": "ePos",
          "choices": {
            "0": "No Climb",
            "1": "Parked",
            "3": "Onstage",
            "5": "Harmony"
          },
          "defaultValue": "0"
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "code": "dt",
