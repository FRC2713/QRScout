          "type": "select",
          "choices": {
            "0": "<15 Seconds",
            "30": "~30 Seconds",
            "60": "~60 Seconds",
            "150": "Whole Match"
          },
          "defaultValue": "0",
          "required": true
        },
        {
          "code": "or",
          "title": "Offense Skill",
          "type": "select",
          "choices": {
            "1": "Ineffective",
            "2": "Below Average",
            "3": "Average",
            "4": "Above Average",
            "5": "Exceptional",
            "x": "Not Observed"
          },
          "defaultValue": "",
          "required": true
        },
        {
          "code": "dr",
          "title": "Defense Skill",
          "type": "select",
          "choices": {
            "1": "Ineffective",
            "2": "Below Average",
            "3": "Average",
            "4": "Above Average",
            "5": "Exceptional",
            "x": "Not Observed"
          },
          "defaultValue": "",
          "required": true
        },
        {
          "code": "yc",
          "title": "Yellow/Red Card",
          "type": "select",
          "defaultValue": "No Card",
          "required": true,
          "choices": {
            "No Card": "No Card",
            "Yellow": "Yellow Card",
            "Red": "Red Card"
          }
        },
        {
          "code": "co",
          "title": "Comments",
          "type": "text",
          "min": 0,
          "max": 50,
          "defaultValue": "",
          "required": false
        }
      ]
    }
