{
  "$schema": "../schema.json",
  "title": "QRScout",
  "page_title": "Crescendo Testing",
  "sections": [
    {
      "name": "Prematch",
      "fields": [
        {
          "title": "Scouter Initials",
          "type": "text",
          "required": true,
          "code": "scouter",
          "preserveDataOnReset": true
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "min": 0,
          "code": "matchNumber",
          "preserveDataOnReset": true,
          "autoIncrementOnReset": true
        },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          },
          "defaultValue": ""
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber"
        },
        {
          "title": "Starting Position",
          "type": "select",
          "required": true,
          "code": "Prsp",
          "choices": {
            "R1": "Source",
            "R2": "Middle",
            "R3": "Amp"
          },
          "defaultValue": ""
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "noShow"
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Leave?",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "Mved"
        },
        {
          "code": "ausc",
          "title": "Speaker Scores",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "auskpm",
          "title": "Speaker Misses",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Amp Scores",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Amp Misses",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "auf",
          "title": "Auto Fouls",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "tamps",
          "title": "Amp Scores",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Amp Misses",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tsc",
          "title": "Speaker Scores",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Speaker Misses",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tfs",
          "title": "Feeder Shots",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "Fou/Tech",
          "title": "Teleop Fouls",
          "type": "counter",
          "defaultValue": 0,
          "required": false
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "code": "cn",
          "title": "Notes in Trap",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "max": 3,
          "required": false
        },
        {
          "title": "End Position",
          "type": "select",
          "required": true,
          "code": "epo",
          "choices": {
            "No": "No Climb",
            "P": "Parked",
            "Os": "Onstage",
            "Hm": "Harmony",
            "Fh": "Failed Harmony"
          },
          "defaultValue": "No"
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "code": "or",
          "title": "Offense Skill",
          "type": "select",
          "choices": {
            "1": "Ineffective",
            "2": "Below Average",
            "3": "Average",
            "4": "Above Average",
            "5": "Exceptional",
            "x": "Not Observed"
          },
          "defaultValue": "",
          "required": true
        },
        {
          "code": "dr",
          "title": "Defense Skill",
          "type": "select",
          "choices": {
            "1": "Ineffective",
            "2": "Below Average",
            "3": "Average",
            "4": "Above Average",
            "5": "Exceptional",
            "x": "Not Observed"
          },
          "defaultValue": "",
          "required": true
        },
        {
          "code": "dto",
          "title": "Died/Tipped Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "yc",
          "title": "Yellow/Red Card",
          "type": "select",
          "defaultValue": "No Card",
          "required": true,
          "choices": {
            "No Card": "No Card",
            "Yellow": "Yellow Card",
            "Red": "Red Card"
          }
        },
        {
          "code": "co",
          "title": "Comments",
          "type": "text",
          "min": 0,
          "max": 50,
          "defaultValue": "",
          "required": false
        }
      ]
    }
  ]
}
